    "@ngrx/store-devtools": "19.0.1",
    "@nx/angular": "20.8.1",
    "express": "^4.18.2",
    "libphonenumber-js": "^1.10.58",
    "lint-staged": "^15.2.2",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
